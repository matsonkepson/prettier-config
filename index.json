{
 "endOfLine": "lf",
 "tabWidth": 3,
 "useTabs": false,
 "singleQuote": false,
 "importOrder": ["^@server/(.*)$", "^@ui/(.*)$", "<THIRD_PARTY_MODULES>", "^[./]"],
 "importOrderSeparation": true,
 "importOrderSortSpecifiers": true,
 "trailingComma": "all",
 "printWidth": 120,
 "overrides": [
  {
   "files": "*.json",
   "options": {
    "tabWidth": 1
   }
  }
 ]
}
